              <div class="col-md-4">
                <!-- Widget -->
                <div class="widget">
                  <!-- Widget title -->
                  <div class="widget-head">
                    <div class="pull-left">All Interviews</div>
                    <div class="widget-icons pull-right">
                      <a href="#" class="wminimize"><i class="fa fa-chevron-up"></i></a> 
                      <a href="#" class="wclose"><i class="fa fa-times"></i></a>
                    </div>  
                    <div class="clearfix"></div>
                  </div>
                  <div class="widget-content">
                    <!-- Widget content -->
                    <div class="padd">
                      <% @interviews.each do |interview| %>
                      
                        <ul class="recent">

                          <li>
                            <div class="recent-content">
                              <div class ="title"><%= interview.name %></div>
                              <div><%= interview.learning %></div>
                              <div class="recent-meta"><%= interview.created_at %></div>
                              
                              <div class="btn-group">
                                <button class="btn btn-xs btn-default"><i class="fa fa-pencil"></i> <%= link_to 'Edit Interview' %> </button>
                        
                              </div>
                              <div class="clearfix"></div>
                            </div>
                          </li>
                                          
                        </ul>
                      <% end %>  
                      

                    </div>
                    <!-- Widget footer -->
                    <div class="widget-foot">

                      
                        <ul class="pagination pagination-sm pull-right">
